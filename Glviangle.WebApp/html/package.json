{
  "name": "webpack-app",
  "version": "2.8.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/app/**",
    "create-router": "node -r dotenv/config createRouter.js dotenv_config_path=env/dev.env",
    "create-router:build": "node -r dotenv/config createRouter.js dotenv_config_path=env/prod.env",
    "start": "npm run create-router && webpack-dev-server --https --config webpack.dev.js --open",
    "start:prod": "npm run create-router:build && webpack-dev-server --https --progress --config webpack.prod.js --open",
    "format": "node directoryFormat",
    "build": "npm run create-router:build && webpack --progress --config webpack.prod.js && npm run format",
    "gulp": "gulp html"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime-corejs2": "^7.6.0",
    "@fancyapps/fancybox": "^3.5.7",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-pug": "^1.2.2",
    "file-loader": "^4.2.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-preprocess": "^3.0.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.5.1",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.15.0",
    "postcss-loader": "^3.0.0",
    "prettier": "2.1.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "select2": "^4.0.12",
    "style-ext-html-webpack-plugin": "^4.1.2",
    "style-loader": "^1.0.0",
    "swiper": "^4.5.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-preprocessor-loader": "^1.1.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@tarekraafat/autocomplete.js": "^7.2.0",
    "aos": "^2.3.4",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "jsrender": "^1.0.7",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0"
  }
}
